<facelet-taglib xmlns="http://java.sun.com/xml/ns/javaee"
                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd"
                version="2.0">
    <namespace>http://www.bkfaces.org/ui</namespace>
    <tag>
        <tag-name>avatar</tag-name>
        <component>
            <component-type>avatar</component-type>
        </component>
        <attribute>
            <description>this is style css dor avatar component</description>
            <name>style</name>
            <required>true</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description>this is value of avatar text</description>
            <name>name</name>
            <required>false</required>
            <type>java.lang.String</type>
        </attribute>
    </tag>


    <tag>
        <tag-name>navBar</tag-name>
        <component>
            <component-type>navBar</component-type>
        </component>
        <attribute>
            <description>this is style css dor avatar component</description>
            <name>style</name>
            <required>true</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description>this is value of avatar text</description>
            <name>appName</name>
            <required>false</required>
            <type>java.lang.String</type>
        </attribute>

    </tag>


</facelet-taglib>